= form_for(setup_resume(@resume)) do |f|
  - if @resume.errors.any?
    #error_explanation
      %h2= pluralize(@resume.errors.count, "error")
      prohibited this resume from being saved:

      %ul
      - @resume.errors.full_messages.each do |msg|
        %li= msg
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :address
    = f.text_field :address
  .field
    = f.label :city
    = f.text_field :city
  .field
    = f.label :state
    = f.text_field :state
  .field
    = f.label :postal_code
    = f.text_field :postal_code
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :phone_number
    = f.text_field :phone_number
  .field
    = f.label :objective
    %br
    = f.text_area :objective, rows: 6, columns: 60
  .actions
    = f.submit
  = f.fields_for :educations do |ed_form|
    .field
      = ed_form.label :university
      = ed_form.text_field :university
