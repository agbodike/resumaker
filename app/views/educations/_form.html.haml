= form_for(@education) do |f|
  - if @education.errors.any?
    #error_explanation
      %h2
        = pluralize(@education.errors.count, "error")
        prohibited this education from being saved:
      %ul
        - @education.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :start_year
    %br/
    = f.text_field :start_year
  .field
    = f.label :end_year
    %br/
    = f.text_field :end_year
  .field
    = f.label :university
    %br/
    = f.text_field :university
  .field
    = f.label :degree
    %br/
    = f.text_field :degree
  .field
    = f.label :major
    %br/
    = f.text_field :major
  .field
    = f.label :resume_id
    %br/
    = f.text_field :resume_id
  .actions
    = f.submit
